        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeroone.star.orgmanager.mapper.OrganizationMapper">
    <select id="selectById" resultType="com.zeroone.star.orgmanager.entity.UOrg">
        SELECT * FROM u_org
        WHERE org_id = #{orgId}
